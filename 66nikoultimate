                   if v.Name == "VIP Door" then
                       if v:IsA("Part") then
                       workspace["VIP Door"]:Destroy()
                   end
                   end
               end
           end)
           for i = 1, 5 do
           pcall(function()
               for i,v in pairs(game.Workspace:GetDescendants()) do
                   if v.Name == "GP Door" then
                       if v:IsA("Part") then
                       workspace["Gamepass Door"]:Destroy()
                   end
                   end
               end
               end)
           end
               
           for i = 1, 5 do
