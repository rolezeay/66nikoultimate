                       if v:IsA("Part") then
                       workspace["Gamepass"]:Destroy()
                   end
                   end
               end
               end)
           end
       
           for i = 1, 5 do
               pcall(function()
                   for i,v in pairs(game.Workspace:GetDescendants()) do
                       if v.Name == "Door" then
                           if v:IsA("Part") then
                           workspace["Door"]:Destroy()
                       end
                       end
                   end
                   end)
               end
       
               end
